/**/
{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "FeatureManagement": {
    "NewFeatureFlag": true,
    "NewPercentageFilter": {
      "EnabledFor": [
        {
          "Name": "Percentage",
          "Parameters": {
            "Value": 50
          }
        }
      ]
    },
    "BrowserFlag": {
      "EnabledFor": [
        {
          "Name": "Browser",
          "Parameters": {
            "Allowed": [ "Edg" ]
          }
        }
      ]
    }

  },
}
